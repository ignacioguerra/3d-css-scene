{
  "name": "3d-css-scene",
  "version": "1.0.5",
  "description": "Create and manipulate a walkable 3d css scene",
  "main": "dist/bundle.js",
  "scripts": {
    "build": "webpack --mode=production",
    "build:watch": "webpack -w --mode=production",
    "start": "webpack-dev-server --open",
    "prepare": "npm run build",
    "publish": "git push origin --tags && git push origin",
    "release:major": "npm version major && npm publish",
    "release:minor": "npm version minor && npm publish",
    "release:patch": "npm version patch && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ignacioguerra/3d-css-scene.git"
  },
  "keywords": [
    "3d",
    "css",
    "space",
    "scene",
    "interactive"
  ],
  "author": "Ignacio Guerra",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/ignacioguerra/3d-css-scene/issues"
  },
  "homepage": "https://github.com/ignacioguerra/3d-css-scene#readme",
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "babel-loader": "^8.1.0",
    "babel-preset-es2015": "^6.24.1",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.0.2",
    "css-loader": "^3.6.0",
    "html-webpack-plugin": "^4.3.0",
    "mini-css-extract-plugin": "^0.9.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "style-loader": "^1.2.1",
    "terser-webpack-plugin": "^3.0.5",
    "url-loader": "^4.1.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "lodash.throttle": "^4.1.1"
  }
}
